    {
        "version": "0.2.0",
        "configurations": [
            {
                "type": "node",
                "request": "launch",
                "name": "Generate Config",
                "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/ts-node",
                "runtimeArgs": [
                    "--transpile-only",
                ],
                "program": "${workspaceFolder}/generateConfig.ts",
                "console": "integratedTerminal",
                "sourceMaps": true,
                "cwd": "${workspaceFolder}",
                "internalConsoleOptions": "openOnSessionStart",
                "skipFiles": [
                    "<node_internals>/**",
                    "node_modules/**"
                ]
            }
        ]
    }